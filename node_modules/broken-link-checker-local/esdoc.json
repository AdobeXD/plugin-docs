{
  "source": "./src",
  "destination": "./docs/api",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "brand": {
          "title": "broken-link-checker-local"
        },
        "access": ["public", "package", "private"],
        "test": {
          "type": "mocha",
          "source": "./test"
        },
        "manual": {
          "files": ["./CHANGELOG.md"]
        }
      }
    }
  ]
}
